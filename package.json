  "description": "sample for LINEBOT message recieve with nuxt, vuetify, express, mongodb, mongoose",
  "author": "testuser",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "devmon": "nodemon --watch api --exec \"nuxt\"",
    "dev-debug": "node --nolazy --inspect-brk=9229 node_modules/nuxt/bin/nuxt",
    "dev-debugapi": "node --nolazy --inspect-brk=9229 api/apptest.js",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test-api": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha api/**/**.test.js",
    "test-api-debug": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha --inspect-brk  api/**/**.test.js",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@line/bot-sdk": "^6.2.1",
    "@nuxtjs/axios": "^5.0.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.3.0",
    "line-login": "^1.0.14",
    "lodash": "^4.17.10",
    "mongodb": "^3.1.3",
    "mongoose": "^5.2.8",
    "nuxt": "^1.4.2",
    "vuetify": "^1.1.12"
  },
  "devDependencies": {
    "cross-env": "^5.0.1",
    "expect": "^23.5.0",
    "mocha": "^5.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "supertest": "^3.1.0"
